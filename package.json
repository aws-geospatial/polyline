{
  "name": "@aws-geospatial/polyline",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/aws-geospatial/polyline",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-geospatial/polyline"
  },
  "bugs": {
    "url": "https://github.com/aws-geospatial/polyline/issues",
    "mail": ""
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "prettier": "prettier -w .",
    "prettier:check": "prettier -c .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "npm-run-all build:*",
    "build:ts": "npm-run-all build-ts:*",
    "build:bundle": "rollup -c",
    "build-ts:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build-ts:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build-ts:esm": "tsc --esModuleInterop --module esnext --outDir dist/esm",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest --collectCoverage --collectCoverageFrom=src/**/*.{ts,js}",
    "posttest": "generate-coverage-data --language typescript",
    "typedoc": "typedoc"
  },
  "main": "./dist/math.js",
  "exports": "./dist/math.js",
  "files": [
    "dist/",
    "!**/__tests__/**"
  ],
  "npm-pretty-much": {
    "runTest": "never"
  },
  "dependencies": {
    "@here/flexpolyline": "^0.1.0",
    "@mapbox/polyline": "^1.2.1",
    "@types/geojson": "^7946.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@eslint/js": "^8.56.0",
    "@jest/types": "^29.5.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.56.0",
    "globals": "^15.6.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rollup": "^4.17.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
